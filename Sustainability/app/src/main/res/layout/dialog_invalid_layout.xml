<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginLeft="@dimen/_30sdp"
    android:layout_marginRight="@dimen/_30sdp"
    android:background="@drawable/selector_invalid_dailog"
    android:layout_height="@dimen/_200sdp"
    android:orientation="vertical">

    <com.gdc.isfacademy.view.customs.customfonts.OpenSansLightTextview
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="@string/app_name"
        android:textSize="@dimen/_15sdp"
        android:layout_margin="@dimen/_15sdp"
        android:textColor="@color/black"
        android:layout_height="wrap_content" />

    <com.gdc.isfacademy.view.customs.customfonts.OpenSansBoldTextView
        android:layout_width="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center_horizontal"
        android:textSize="@dimen/_20sdp"
        android:text="Enable to Open app ISF Sustainability "
        android:layout_height="wrap_content" />


    <com.gdc.isfacademy.view.customs.customfonts.OpenSansLightTextview
        android:layout_width="wrap_content"
        android:id="@+id/cancelBtn"
        android:text="@string/txt_cancel"
        android:textColor="@color/white"
        android:layout_marginBottom="@dimen/_10sdp"
        android:paddingTop="@dimen/_2sdp"
        android:paddingBottom="@dimen/_2sdp"
        android:textSize="@dimen/_15sdp"
        android:paddingLeft="@dimen/_30sdp"
        android:paddingRight="@dimen/_30sdp"
        android:background="@drawable/blue_rect_crop_bg"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_height="wrap_content" />




</RelativeLayout>